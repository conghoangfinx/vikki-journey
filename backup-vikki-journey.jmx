    amount: 490000000,
    refNo: vars.get(&quot;refNo&quot;),
    currency: &quot;VND&quot;,
    transmissionDateTime: vars.get(&quot;timestamp&quot;),
    fromAccount: &quot;0421000487800&quot;,
